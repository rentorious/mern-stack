{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./dist",
      "esModuleInterop": true,
"moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "strict": true,
      "noImplicitAny": false,
      "baseUrl": "./src"
    },
    "include": ["src"],

  }
  